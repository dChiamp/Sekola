{
  "manifest_version": 2,

  "name": "Sekola",
  "description": "vanilla js timer",
  "version": "1.0",

  "browser_action": {
    "default_icon": "clock.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
